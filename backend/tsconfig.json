{
    "compilerOptions": {
        "target": "ES2020",
        "module": "commonjs",
        "lib": [
            "ES2020"
        ],
        "outDir": "./dist",
        "rootDir": "./",
        "strict": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "sourceMap": true,
        "noImplicitAny": false,
        "strictNullChecks": false
    },
    "include": [
        "src/**/*",
        "prisma/seed.ts"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}